
/*
init: sql 初始化 sql Initialization
exec: 执行 SQL 语句 Executing SQL statements
sql: Chain Call Generate SQL 语句 Chain Call Generates SQL Statement
transaction: 事务 API transaction API
*/

init(config.DATABASE_INIT)



async function test(){
    const SQL0 = sql.table('atom_project')
        .field('*')
        .where({id:2})
        .select()

    console.log(SQL0)
    const SQL = await exec(SQL0)
//
    console.log(SQL)
}

test()